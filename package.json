{
    "name": "namespace-autocompletion",
    "displayName": "C# Namespace Autocompletion, support snake_case to PascalCase",
    "description": "Full namespace autocompletion for C#",
    "publisher": "linyuansup",
    "repository": {
        "type": "git",
        "url": "https://github.com/linyuansup/NamespaceAutocompletion.git"
    },
    "icon": "img/logo.png",
    "version": "1.2.0",
    "engines": {
        "vscode": "^1.32.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "keywords": [
        "C#",
        "CSharp",
        "Namespace",
        "Autocompletion",
        "Autocomplete",
        "IntelliSense",
        ".NET",
        "Godot",
        ".NET Core"
    ],
    "activationEvents": [
        "onLanguage:csharp"
    ],
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^3.3.1",
        "vscode": "^1.1.28",
        "tslint": "^5.12.1",
        "@types/node": "^10.12.21",
        "@types/mocha": "^2.2.42"
    }
}